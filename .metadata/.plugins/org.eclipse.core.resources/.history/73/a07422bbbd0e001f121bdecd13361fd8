		// TODO Auto-generated method stub

		
//		for(int i=0; i<10; i++) {
//			for(int j=0; j<=i; j++) {
//				System.out.print("*");
//			}
//			System.out.println();
//		}
		
		
		
		int i = 0;
		while(i<10) {
			System.out.println(i);
			i++;
		}
		
		
		int j = 0;
		while(true) {
			System.out.println(j);
			if(j == 5) continue;
			
			if(j>10) break;
			
			j++;
		}
		
		
	}

}
